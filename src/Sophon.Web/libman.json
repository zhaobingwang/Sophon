{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "provider": "unpkg",
      "library": "echarts@4.9.0",
      "destination": "wwwroot/lib/echarts/",
      "files": [
        "dist/extension/bmap.js",
        "dist/extension/bmap.js.map",
        "dist/extension/bmap.min.js",
        "dist/extension/dataTool.js",
        "dist/extension/dataTool.js.map",
        "dist/extension/dataTool.min.js",
        "dist/echarts-en.common.js",
        "dist/echarts-en.common.min.js",
        "dist/echarts-en.js",
        "dist/echarts-en.js.map",
        "dist/echarts-en.min.js",
        "dist/echarts-en.simple.js",
        "dist/echarts-en.simple.min.js",
        "dist/echarts.common.js",
        "dist/echarts.common.min.js",
        "dist/echarts.js",
        "dist/echarts.js.map",
        "dist/echarts.min.js",
        "dist/echarts.simple.js",
        "dist/echarts.simple.min.js"
      ]
    },
    {
      "provider": "unpkg",
      "library": "@microsoft/signalr@3.1.9",
      "destination": "wwwroot/lib/microsoft/signalr/",
      "files": [
        "dist/browser/signalr.js",
        "dist/browser/signalr.min.js"
      ]
    },
    {
      "provider": "unpkg",
      "library": "toastr@2.1.4",
      "destination": "wwwroot/lib/toastr/",
      "files": [
        "build/toastr.css",
        "build/toastr.js.map",
        "build/toastr.min.css",
        "build/toastr.min.js"
      ]
    }
  ]
}